{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "andreaselia/laravel-api-to-postman": "^1.7",
        "artesaos/seotools": "^0.20.0",
        "consoletvs/charts": "^7.2",
        "cviebrock/eloquent-sluggable": "^8.0",
        "cviebrock/eloquent-taggable": "^8.0",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "jackiedo/dotenv-editor": "^1.2",
        "kutia-software-company/larafirebase": "^1.3",
        "laravel/framework": "^8.12",
        "laravel/passport": "^10.1",
        "laravel/tinker": "^2.5",
        "maatwebsite/excel": "^3.1",
        "mtvs/eloquent-hashids": "^3.1",
        "optix/media": "^0.5.0",
        "proengsoft/laravel-jsvalidation": "^4.4",
        "pusher/pusher-php-server": "^6.1",
        "spatie/laravel-activitylog": "^3.17",
        "spatie/laravel-permission": "^4.0",
        "yajra/laravel-datatables": "^1.5",
        "yoeunes/toastr": "^1.2"
    },
    "require-dev": {
        "beyondcode/laravel-er-diagram-generator": "^1.5",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.5",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "autoload": {
        "files": [
            "app/Helpers/helper.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            	"laravel/telescope"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
